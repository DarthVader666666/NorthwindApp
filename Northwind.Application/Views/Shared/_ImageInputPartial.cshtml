@using Northwind.Application.Models.Category
@using Northwind.Application.Models.Seller
@model object

@{
    if (Model is SellerEditModel)
    {
        <input type="hidden" asp-for=@(((SellerEditModel)Model).Photo) />
    }

    if (Model is CategoryEditModel)
    {
        <input type="hidden" asp-for=@(((CategoryEditModel)Model).Picture) />
    }
}
